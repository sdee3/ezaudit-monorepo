  },
  "homepage": "https://github.com/sdee3/ezaudit-monorepo#readme",
  "license": "MIT",
  "name": "ezaudit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdee3/ezaudit-monorepo.git"
  },
  "private": true,
  "scripts": {
    "dev": "concurrently \"php artisan serve\" \"php artisan queue:work\" \"cd client && pnpm dev\"",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "mix --production",
    "build": "concurrently \"cd client && pnpm build && pm2 start pnpm --interpreter bash -- start\"",
    "preinstall": "npx only-allow pnpm",
    "cy:open": "concurrently \"cd client && pnpm cy:open\"",
    "cy:run": "concurrently \"cd client && pnpm cy:run\""
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "laravel-mix": "^6.0.6",
    "resolve-url-loader": "^3.1.2"
  },
  "version": "0.0.1",
  "dependencies": {
    "postcss": "8"
  }
}
